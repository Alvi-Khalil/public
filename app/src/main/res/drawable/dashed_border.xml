<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:bottom="-5dp"
        android:left="0dp"
        android:right="-5dp"
        android:top="-5dp">

        <shape android:shape="rectangle">
            <stroke
                android:width="1dp"
                android:color="#ffff"
                android:dashGap="3dp"
                android:dashWidth="2dp" />

            <solid android:color="@android:color/transparent" />


        </shape>
    </item>
</layer-list>